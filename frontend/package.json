    "electron-is-dev": "^2.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "concurrently": "^8.0.0",
    "electron": "^26.0.0",
    "vite": "^5.0.0",
    "wait-on": "^7.0.0"
  }
}
