{
  "name": "docflow-frontend",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "main": "electron-main.cjs",
  "scripts": {
    "dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
